{
  "name": "issue_token",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "build": "npm run ele:build && vue-cli-service build --mode production --modern",
    "lint": "vue-cli-service lint",
    "ele:build": "rm -rf ele-theme && ./node_modules/.bin/et -o ele-theme",
    "inspect": "vue-cli-service inspect"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "element-ui": "^2.4.6",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@vue/cli-plugin-babel": "^3.0.2",
    "@vue/cli-plugin-eslint": "^3.0.2",
    "@vue/cli-service": "^3.0.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.11.0",
    "babel-preset-env": "^1.7.0",
    "compression-webpack-plugin": "^2.0.0",
    "element-theme": "^2.0.1",
    "element-theme-chalk": "^2.4.6",
    "element-theme-default": "^1.4.13",
    "eslint": "^5.11.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "thorify": "^1.0.4",
    "url-loader": "^1.1.2",
    "vue-template-compiler": "^2.5.17",
    "web3": "^1.0.0-beta.37",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "arrow-parens": 0,
      "generator-star-spacing": 0,
      "no-debugger": 2,
      "space-before-function-paren": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 4 versions",
    "not ie <= 8"
  ]
}
